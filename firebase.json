{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "/",
    "redirects": [ {
      "source": "/",
      "destination": "/login.html",
      "type": 301
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
